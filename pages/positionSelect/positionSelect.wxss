@import '../../assets/styles/font.wxss';
@import '../../assets/styles/flex.wxss';

.section {
  background-color: #fff;
  width: 100%;
  display: flex;
  flex-direction: column;
  padding-bottom: 72rpx;
}

.section .header {
  background-color: #fff;
  padding: 0 32rpx;
  flex: 0;
}

.section .header .search-bar-wrapper {
  height: 68rpx;
  border-radius: 34rpx;
  width: 100%;
  background-color: #f9f9f9;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
}

.section .header .search-bar-wrapper .placeholder {
  font-size: 24rpx;
  color: #bebebe;
}

.section .header .search-bar-wrapper .uniE908 {
  color: #9b9b9b;
  font-size: 28rpx;
  margin-left: 40rpx;
  margin-right: 16rpx;
}

.section .position-box {
  padding: 0 40rpx;
  position: relative;
}

.section .position-box .position-box-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #1d1d1d;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  margin-top: 28rpx;
}

.section .position-box .positon-box-clear {
  position: absolute;
  top: 0;
  right: 40rpx;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  margin-top: 28rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #f1612a;
}

.section .position-box .positon-box-spread {
  position: absolute;
  top: 0;
  right: 40rpx;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  margin-top: 28rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #1d1d1d;
}

.section .position-box .positon-box-spread .uniE90C {
  display: inline-block;
  transform: rotate(90deg);
  margin-left: 10rpx;
}

.section .position-box .positon-box-spread.hold .uniE90C {
  transform: rotate(-90deg);
}

.section .position-box .position-list {
  padding-left: 26rpx;
  height: 148rpx;
  max-height: 148rpx;
  overflow: hidden;
}

.section .position-box .position-list.hot-city {
  height: auto;
  max-height: 148rpx;
}

.section .position-box .position-list.hold {
  max-height: 290rpx;
  height: 290rpx;
}

.section .position-box .position-list.unless {
  max-height: unset;
  height: auto;
}


.section .position-box .position-list .flex-box {
  display: flex;
  flex-wrap: wrap;
}

.changeTab {
  flex: 0 0 auto;
  align-items: flex-end;
  color: #f1612a;
  font-size: 20rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  text-align: right;
  padding-right: 30rpx;
  background-size: 22rpx 22rpx;
}

.section .position-box .position-list .item {
  flex: 0 0 auto;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  padding: 0 20rpx;
  border-radius: 54rpx;
  background-color: #f9f9f9;
  font-size: 28rpx;
  color: #171717;
  font-weight: 400;
  margin-bottom: 20rpx;
  margin-right: 24rpx;
}

.section .position-box .position-list {
  display: flex;
  flex-wrap: wrap;min-height: unset;
height: auto;

}

.section .position-box .position-list .item.history {
  max-width: 188rpx;
  height: 27px;
  line-height: 27px;
  border-radius: 27px;
  background-color: #f9f9f9;
  font-size: 12px;
  color: #171717;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 10px;
  margin-right: 22px;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  display: -webkit-box;
  text-align: center;
}

.section .position-box .position-list .item.hot-city {
  max-width: 240rpx;
  height: 27px;
  line-height: 27px;
  border-radius: 27px;
  background-color: #FFF5EB;
  font-size: 12px;
  color: #171717;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 10px;
  margin-right: 12px;
}

.hot-image {
  width: 14px;
  height: 13px;
  margin-right: 6px;
  margin-bottom: 5px;
}

.section .position-box .position-list .item.hot-city .text {
  /* max-width: 200rpx; */
  flex: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

