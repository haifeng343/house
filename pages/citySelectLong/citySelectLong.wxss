@import '../../assets/styles/font.wxss';
@import '../../assets/styles/flex.wxss';

.page {
  height: 100%;
  background-color: #f9f9f9;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.page .section {
  width: 100%;
  margin-top: 20rpx;
  background-color: #fff;
  padding: 0 32rpx;
  box-sizing: border-box;
  height: auto;
}

.page .section .title {
  font-size: 32rpx;
  font-weight: 400;
  color: #1d1d1d;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  margin-top: 28rpx;
}

.page .section .title.position-title {
  font-size: 28rpx;
  color: #bec3cd;
}

.page .section .position {
  display: flex;
  flex-direction: column;
  padding-bottom: 12rpx;
  border-bottom: 2rpx solid rgba(232,232,232,0.51);
}

.page .section .position .pos,
.page .section .position .repos {
  color: #f1612a;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
}
.page .section .position .repos{
  margin-right: 38rpx;
  width: 50%;
  box-sizing: border-box;
  text-align: right;
}

.page .section .position .pos {
  padding-left: 40rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 32rpx 32rpx;
  width: 50%;
  box-sizing: border-box;
}

.page .section .total {
  font-size:28rpx;
  font-weight:400;
  color:rgba(190,195,205,1);
  line-height:40rpx;
  margin-top: 24rpx;
}

.page .city-list {
  box-sizing: border-box;
  flex: 1;
  overflow: scroll;
  display: flex;
  flex-direction: column;
  background-color: #fff;
}


.page .city-list .city {
  box-sizing: border-box;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0 32rpx;
  border-bottom: solid 2rpx rgba(232,232,232,0.5);
}

.page .city-list .city:last-child {
  margin-bottom: 20rpx;
}

.city .name-icon {
  padding: 0 16rpx;
  font-size: 18rpx;
  color: rgba(190,195,205,1);
}

.city .name-text {
  flex: 1;
  font-size: 30rpx;
  color: #171717;
}

.city .icon-font {
  color: #bec3cd;
  font-size: 28rpx;
}