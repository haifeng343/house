@import "../../assets/styles/font.wxss";
@import "../../assets/styles/flex.wxss";

/* pages/search/search.wxss */

.section {
  height: 100%;
  background: #f9f9f9;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.banner {
  width: 750rpx;
  height: 468rpx;
  position: relative;
  z-index: 1;
}

.banner-image {
  width: 750rpx;
  height: 468rpx;
  background-color: #ccc;
}

.content {
  /* position: relative; */
  /* overflow: hidden; */
  margin: -240rpx auto 20rpx;
  width: 710rpx;
  flex: 1;
  z-index: 2;
  border-bottom-left-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
  /* margin-bottom: 10px; */
  box-shadow: 6rpx 10rpx 20rpx 0rpx rgba(0, 0, 0, 0.07);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.search-box {
  width: 100%;
  /* height: calc(100% - 150rpx); */
  flex: 1;
  margin: 0 auto;
  border-radius: 6rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  position: relative;
  padding-bottom: 140rpx;
}

.nav {
  width: 100%;
  display: flex;
  align-items: flex-end;
  background-color: transparent;
}

.nav .nav_item {
  flex: 1 1 auto;
  height: 40px;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 3;
}

.nav .nav_item .nav_item_text {
  position: relative;
}

.nav .nav_item.active {
  width: 133px;
  flex: 0 0 auto;
  color: #e75d28;
  height: 46px;
  margin-top: 0;
  position: relative;
  z-index: 2;
  font-weight: bold;
  background-color: transparent;
  background-repeat: no-repeat;
  background-size: contain;
}

.nav .nav_item.short.active {
  background-image: url("data:image/svg+xml;base64,");
}

.nav .nav_item.long.active {
  width: 140px;
  background-image: url("data:image/svg+xml;base64,");
}
.nav .nav_item.active .nav_item_text::before {
  content: "";
  display: block;
  width: 12rpx;
  height: 18rpx;
  position: absolute;
  top: 50%;
  left: -26rpx;
  background: url(data:image/png;base64,)
    no-repeat;
  background-size: 12rpx 18rpx;
  transform: translate(0, -50%);
}
.nav .nav_item.active .nav_item_text::after {
  content: "";
  display: block;
  width: 12rpx;
  height: 18rpx;
  position: absolute;
  top: 50%;
  right: -26rpx;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAYAAAAJCAYAAAARml2dAAAAAXNSR0IArs4c6QAAAGlJREFUCB1jZEACdwLdxBj+/VD58//vZSYkcYY/f78K/mf8x8bFxcXLdC/YVg+EkRWA2Cx///4XQhcE8VGMQlaAU4KFkYnxN7JKGJvlw2+B8zAOMs1isnnzN5gAC/P/9/8ZWIQ4/v7+BADKzB9OvedWrgAAAABJRU5ErkJggg==)
    no-repeat;
  transform: translate(0, -50%) rotate(180deg);
}
.search-content {
  width: 100%;
  flex: 1;
  background-color: #fff;
  padding: 0;
  box-sizing: border-box;
  position: relative;
}
.search-content-main {
  width: 100%;
  height: 100%;
  overflow-y: auto;
  padding: 0;
  box-sizing: border-box;
  opacity: 1;
  position: absolute;
  top: 0;
  left: 0;
  transition: opacity 0.7s ease-in-out;
  z-index: 1;
}
.search-content-main.hidden {
  opacity: 0;
  z-index: -1;
}
.search_item {
  height: 108rpx;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  position: relative;
}
.search-content .search_item {
  margin: 0 32rpx;
  box-sizing: border-box;
}
.search_item--spread {
  color: #e75d28;
  text-align: center;
  margin: 12rpx 0;
}
.search_item--spread .icon-font {
  margin-top: 10rpx;
  font-size: 24rpx;
}
.search_item .sort {
  position: absolute;
  top: 20rpx;
  right: 0;
  font-size: 24rpx;
}
.search_item .sort .uniE930 {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  color: #fff;
  line-height: 28rpx;
  font-size: 18rpx;
  text-align: center;
  background-color: #fff;
  border: 2rpx solid #e75d28;
  border-radius: 8rpx;
}
.search_item .sort .uniE930.active {
  color: #fff;
  background-color: #e75d28;
}
.search_item.noBorder {
  border: none;
  height: auto;
  margin-bottom: 50rpx;
  justify-content: flex-start;
  padding-top: 20rpx;
}
.search_item .position {
  flex: 1;
  font-size: 32rpx;
  color: #1d1d1d;
  line-height: 108rpx;
  font-weight: bold;
}
.search_item .positionByHand {
  flex: 0 0 auto;
  width: 234rpx;
  line-height: 108rpx;
  font-size: 24rpx;
  color: #1d1d1d;
  text-align: right;
  padding-right: 36rpx;
  box-sizing: border-box;
  background: url(data:image/png;base64,)
    no-repeat center right;
  background-size: 28rpx 36rpx;
  margin-right: 12rpx;
}
.search_item .inDate {
  flex: 0 0 auto;
  width: 186rpx;
}
.search_item .outDate {
  flex: 0 0 auto;
  width: 156rpx;
}
.search_item .total {
  flex: 1;
}
.search_item .number {
  flex: 0 0 auto;
  width: 154rpx;
  text-align: center;
}
.search_item .split {
  flex: 0 0 auto;
  width: 2rpx;
  height: 60rpx;
  align-items: center;
  background-color: #f4f4f4;
}
.search_item .inDate .text,
.search_item .outDate .text,
.search_item .number .text {
  font-size: 20rpx;
  color: #bec3cd;
  line-height: 28rpx;
}
.search_item .inDate .date,
.search_item .outDate .date,
.search_item .number .num {
  font-size: 32rpx;
  color: #1d1d1d;
  line-height: 44rpx;
  font-weight: bold;
}
.search_item .number .num {
  font-size: 28rpx;
}
.search_item .scenic {
  flex: 1;
  font-size: 32rpx;
  color: #bec3cd;
  background: url(data:image/png;base64,)
    no-repeat 96% center;
  background-size: 14rpx 24rpx;
  padding-right: 60rpx;
  text-align: justify;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.search_item .scenic.active {
  color: #1d1d1d;
}
.search_item .title {
  width: 100%;
  font-size: 32rpx;
  padding-left: 28rpx;
  font-weight: bold;
  margin-bottom: 22rpx;
}
.search_item .title.icon_1 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
  margin-bottom: 0;
}
.search_item .title.icon_2 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
}
.search_item .houseType {
  flex: 0 0 auto;
  width: 136rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background: #f0f0f0;
  text-align: center;
  line-height: 40rpx;
  margin-right: 22rpx;
}
.search_item .houseType.active {
  background-color: #f1612a;
  color: #fff;
}
.search_item .title.icon_3 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
}
.search_item .leaseType {
  flex: 0 0 auto;
  width: 156rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background: #f0f0f0;
  text-align: center;
  line-height: 40rpx;
  margin-right: 34rpx;
}
.search_item .leaseType.active {
  background-color: #f1612a;
  color: #fff;
}
.search_item .title.icon_4 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
}
.search_item .equipments {
  font-size: 28rpx;
  color: #1d1d1d;
  width: 25%;
  text-align: center;
  margin-bottom: 18rpx;
}
.search_item .equipments .equipments-img {
  background: #fff6f2;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin: 0 auto 6rpx;
  color: #1d1d1d;
  font-size: 46rpx;
  text-align: center;
  line-height: 96rpx;
}
.search_item .equipments.active .equipments-img {
  background: #f1612a;
  color: white;
}
.search-btn-back {
  position: absolute;
  width: 710rpx;
  height: 146rpx;
  left: 0;
  bottom: 0;
  padding-top: 30rpx;
  box-sizing: border-box;
  background: white;
  border-bottom-left-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
}
.search-btn {
  width: 480rpx;
  height: 92rpx;
  line-height: 92rpx;
  background: linear-gradient(
    180deg,
    rgba(231, 93, 40, 1) 0%,
    rgba(233, 68, 68, 1) 100%
  );
  box-shadow: 2px 2px 4px 0px rgba(198, 55, 35, 0.48);
  border-radius: 92px;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  margin: 0 auto;
}
.select-button {
  width: 312rpx;
  height: 96rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 6rpx;
  border: 2rpx solid rgba(243, 243, 243, 1);
  padding: 10rpx 0 10rpx 42rpx;
  box-sizing: border-box;
  margin: 34rpx 0 6rpx;
}
.select-button.active {
  background: rgba(255, 214, 198, 1);
  border: 2rpx solid rgba(241, 97, 42, 1);
}
.select-button .select-button-title {
  font-size: 28rpx;
  font-weight: 600;
  color: rgba(29, 29, 29, 1);
  line-height: 40rpx;
}
.select-button .select-button-info {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(190, 195, 205, 1);
  line-height: 34rpx;
}
.select-button.active .select-button-info {
  color: rgba(241, 97, 42, 1);
}
