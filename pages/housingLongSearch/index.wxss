/* pages/housingSearch/housingSearch.wxss */
@import '../../assets/styles/font.wxss';
@import '../../assets/styles/flex.wxss';
@import '../housingSearch/housingSearch.wxss';

.child-item {
  width: 686rpx;
  margin: 0 auto;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center; 
}
.child-item-type {
  width:68rpx;
  height:34rpx;
  background:rgba(255,243,239,1);
  border-radius:4rpx;
  margin-right: 12rpx;
  font-size:12px;
  font-weight:400;
  color:rgba(241,97,42,1);
  line-height:34rpx;
  text-align: center;
}
.child-item-name {
  height: 100rpx;
  flex: 1;
  font-size:28rpx;
  font-weight:400;
  color:rgba(29,29,29,1);
  line-height:100rpx;
  border-bottom:2rpx solid #F4F4F4;
  box-sizing: border-box;
  background: url(data:image/png;base64,)
    no-repeat 96% center;
  background-size: 14rpx 24rpx;
  padding-right: 60rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}