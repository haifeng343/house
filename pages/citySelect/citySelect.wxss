@import '../../assets/styles/font.wxss';
@import '../../assets/styles/flex.wxss';

.section {
  height: 100%;
}

.section .page {
  background-color: #f9f9f9;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.section .page .index-list {
  position: fixed;
  display: flex;
  flex-direction: column;
  right: 0;
  z-index: 9;
  top: 240rpx;
  height: calc(100% - 260rpx);
  justify-content: space-around;
}

.section .page .index-list .index-item {
  font-size: 24rpx;
  font-weight: 400;
  color: #bec3cd;
  height: 40rpx;
  line-height: 40rpx;
  flex: 0 0 auto;
  padding: 0 20rpx;
}

.section .page .header {
  background-color: #fff;
  padding: 0 32rpx;
  flex: 0;
}

.section .page .header .search-bar-wrapper {
  height: 68rpx;
  border-radius: 34rpx;
  width: 100%;
  background-color: #f9f9f9;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
}

.section .page .header .search-bar-wrapper .placeholder {
  font-size: 24rpx;
  color: #bebebe;
}

.section .page .header .search-bar-wrapper .uniE908 {
  color: #9b9b9b;
  font-size: 28rpx;
  margin-left: 40rpx;
  margin-right: 16rpx;
}

.section .page .header .tab-wrapper {
  width: 60%;
  margin: 0 auto 20rpx auto;
}

.section .page .header .tab-wrapper .tab-item {
  line-height: 56rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #1d1d1d;
}

.section .page .header .tab-wrapper .tab-item.active {
  border-bottom: solid 4rpx #f1612a;
  color: #f1612a;
}

.section .page .city-list {
  padding-top: 20rpx;
  box-sizing: border-box;
  flex: 1;
  overflow: scroll;
  display: flex;
  flex-direction: column;
  height: calc(100% - 220rpx);
}

.section .page .city-list .section {
  background-color: #fff;
  padding: 0 40rpx;
  float: 0;
  height: auto;
}

.section .page .city-list .section .title {
  font-size: 32rpx;
  font-weight: 400;
  color: #1d1d1d;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  margin-top: 28rpx;
}

.section .page .city-list .section .title.position-title {
  font-size: 28rpx;
  color: #bec3cd;
}

.section .page .city-list .section .title.hot-title {
  font-weight: bold;
  padding-left: 30rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 24rpx 24rpx;
}

.section .page .city-list .section .title.history-title {
  font-weight: bold;
  padding-left: 30rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 24rpx 24rpx;
}

.section .page .city-list .section .position {
  display: flex;
  flex-direction: column;
}

.section .page .city-list .section .position .pos,
.section .page .city-list .section .position .repos {
  color: #f1612a;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
}
.section .page .city-list .section .position .repos{
  margin-right: 38rpx;    
}

.section .page .city-list .section .position .pos {
  padding-left: 40rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 32rpx 32rpx;
}

.section .page .city-list .section .position .pos .uniE909,
.section .page .city-list .section .position .repos .uniE909 {
  font-size: 24rpx;
  margin-right: 12rpx;
}

.section .page .city-list .section .hot-city {
  padding-bottom: 20rpx;
}

.section .page .city-list .section .search-history {
  position: relative;
  padding-top: 88rpx;
}

.section .page .city-list .section .search-history .title {
  position: absolute;
  top: 0;
  left: 0;
  margin-bottom: 20rpx;
  margin-top: 28rpx;
}

.section .page .city-list .section .search-history .clear {
  position: absolute;
  top: 0;
  right: 40rpx;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  margin-top: 28rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #f1612a;
  padding-right: 26rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 22rpx 24rpx;
}

.section .page .city-list .section .cell-list {
  flex-wrap: wrap;
  padding-left: 26rpx;
}

.section .page .city-list .section .cell-list .cell-item {
  width: 188rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  background-color: #f9f9f9;
  font-size: 28rpx;
  color: #171717;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 20rpx;
  margin-right: 26rpx;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  display: -webkit-box;
  text-align: center;
}

.hot-image {
  width: 28rpx;
  height: 36rpx;
  margin-right: 12rpx;
}

.section .page .city-list .section .cell-list .cell-item.hot {
  background-color: #faf0e6;
}

.section .page .city-item .title {
  background-color: #f9f9f9;
  width: 100%;
  display: flex;
  height: 48rpx;
  align-items: center;
  padding-left: 32rpx;
  font-size: 28rpx;
  color: #bec3cd;
  box-sizing: border-box;
}

.section .page .city-item .content {
  background-color: #fff;
}

.section .page .city-item .content .city {
  box-sizing: border-box;
  height: 80rpx;
  display: flex;
  align-items: center;
  margin: 0 28rpx;
  padding-left: 52rpx;
  font-size: 32rpx;
  color: #171717;
}

.section .page .city-item .content .city+.city {
  border-top: solid 2rpx #e8e8e8;
}
