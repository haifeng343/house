@import '../../assets/styles/font.wxss';

:host {
  display: flex;
  width: 100%;
}

.container {
  width: 100%;
  height: 256rpx;
  display: flex;
  align-items: center;
  position: relative;
}

.area {
  width: calc(100% - 70px);
  height: 256rpx;
}

.delete-button {
  height: 256rpx;
  width: 70px;
  background-color: transparent;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 44rpx;
  color: #bec3cd;
}

.delete-button .uniE907 {
  color: #f25555;
}

.wrapper {
  background-color: #fff;
  padding-left: 68rpx;
  padding-right: 40rpx;
  width: calc(100vw - 40rpx);
  height: 256rpx;
  box-sizing: border-box;
  border: 2rpx solid rgba(214, 214, 214, 0.19);
  box-shadow: 4rpx 4rpx 10rpx 0 rgba(0, 0, 0, 0.16);
  border-radius: 12rpx;
  position: relative;
  overflow: hidden;
  z-index: 2;
}

.top {
  padding-top: 36rpx;
  height: 148rpx;
  box-sizing: border-box;
  border-bottom: solid 1rpx #f4f4f4;
}

.top-item1 {
  display: flex;
  align-items: flex-start;
  color: rgba(192, 199, 208, 1);
}

.top-item1-text1 {
  font-size: 44rpx;
  font-weight: 500;
  line-height: 60rpx;
  width: 410rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: break-all;
}

.top-item1-text2 {
  display: flex;
  align-items: center;
  margin-left: auto;
}

.top-item1-text2-label {
  font-size: 28rpx;
  font-weight: 400;
}

.top-item1-text2-value {
  font-size: 44rpx;
  font-weight: 500;
}

.top-item2 {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(190, 195, 205, 1);
  line-height: 36rpx;
}

.bottom {
  height: 100rpx;
  box-sizing: border-box;
}

.bottom-item1 {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(190, 195, 205, 1);
  line-height: 36rpx;
  margin-top: 28rpx;
}

.tag {
  width: 96rpx;
  height: 90rpx;
  background-image: url(data:image/svg+xml;base64,);
  background-repeat: no-repeat;
  background-size: 96rpx 90rpx;
  position: absolute;
  top: 9rpx;
  left: 9rpx;
}
.long_tag{
  background-image: url(data:image/png;base64,)
}
.bg {
  height: 256rpx;
  width: 714rpx;
  background-image: url(data:image/svg+xml;base64,);
  background-repeat: no-repeat;
  background-size: 714rpx 256rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.uniE929 {
  position: absolute;
  font-size: 136rpx;
  color: #d8d8d8;
  right: 60rpx;
  top: 60rpx;
  z-index: 9;
}
