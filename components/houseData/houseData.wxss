.house_query{
  margin: 0 28rpx 20rpx;display: flex;align-items: center;color: #1D1D1D;
}
.house_query .query{
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.house_data{margin: 0 28rpx;border-bottom: 1rpx solid #E8E8E8;padding-bottom: 20rpx}
.house_data .data_first,.house_data .data_sencond{
  display: flex;
  color: #1D1D1D;
}
.house_data .data_first{
  margin-bottom: 10rpx;
}
.data_first .data_layout,.data_sencond .data_layout{
  flex: 1;
  height: 128rpx;
  padding: 18rpx 0 0 34rpx;
  background-repeat:no-repeat; 
  background-size:100% 100%;
  background-image:url('data:image/png;base64,');
}
.data_first .data_layout:first-child,.data_sencond .data_layout:first-child{
  margin-right: 10rpx;
}
.data_layout .real_num{font-size: 48rpx;color: #F8685F}
.data_layout .average_price{font-size: 48rpx;color: #F0B258}
.data_layout .total_num{font-size: 48rpx;color: #63C559}
.data_layout .low_price{font-size: 48rpx;color: #79AAFF}
.data_first .data_average{
  background-image:url('data:image/png;base64,');
}
.data_sencond .data_total{
  background-image:url('data:image/png;base64,');
}
.data_sencond .data_low{
  background-image:url('data:image/png;base64,');
}