.container {
  background-color: #fff;
  display: flex;
  width: 100%;
}

.icon {
  width: 98rpx;
  height: 160rpx;
  flex: 0 0 auto;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: 46rpx 46rpx;
}

.icon.icon1 {
  background-image: url(data:image/png;base64,);
}

.icon.icon2 {
  background-image: url(data:image/png;base64,);
}

.icon.icon3 {
  background-image: url(data:image/png;base64,);
}

.info {
  width: calc(100% - 98rpx);
  display: flex;
  flex-direction: column;
  border-bottom: 2rpx solid rgba(232, 232, 232, 0.5);
}

.box-wrapper {
  padding: 32rpx 0;
  display: flex;
  justify-content: space-between;
  width: 100%;
}

.info .box {
  white-space: nowrap;
  display: flex;
  flex-direction: column;
}

.box-left {
  overflow: hidden;
  flex: 1 1 auto;
}

.box-right {
  flex: 0 0 auto;
}

.info .name {
  color: #1d1d1d;
  font-size: 32rpx;
  margin-bottom: 12rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
}

.info .platform {
  color: #e75d28;
  font-size: 24rpx;
}

.info .platform .number {
  color: #1d1d1d;
}

.info .money {
  color: #1d1d1d;
  font-size: 36rpx;
  text-align: right;
  padding-right: 28rpx;
  margin-bottom: 12rpx;
}

.info .money.fu {
  color: #f25555;
}

.info .date {
  color: #bec3cd;
  font-size: 24rpx;
  text-align: right;
  padding-right: 34rpx;
}

.check {
  color: #f1612a;
  font-size: 24rpx;
  margin-bottom: 20rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
  width: 100%;
  text-align: right;
}

.info .pay-list {
  width: 100%;
  margin-top: 36rpx;
}

.info .pay-list .item {
  display: flex;
  justify-content: space-between;
}

.info .pay-list .item .date {
  color: #bec3cd;
  font-size: 24rpx;
  text-align: left;
}

.info .pay-list .item .type {
  color: #1d1d1d;
  font-size: 24rpx;
}

.info .pay-list .item .money {
  color: #f25555;
  font-size: 28rpx;
  text-align: right;
  margin-bottom: 14rpx;
}

.info .hold {
  width: 100%;
  height: 24rpx;
  background: url(data:image/png;base64,)
    no-repeat center center;
  margin-top: 12rpx;
}

.vhidden {
  display: none;
}
