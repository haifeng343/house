.banner {
  width: 750rpx;
  height: 440rpx;
  position: relative;
  border: 1px solid #000;
  box-sizing: border-box;
}

swiper {
  width: 750rpx;
  height: 440rpx;
}

swiper-item {
  width: 750rpx;
  height: 440rpx;
  text-align: center;
}

swiper-item image {
  width: 750rpx;
  height: 440rpx;
}

.box {
  width: 100%;
  height: 100%;
  display: inline-block;
  box-sizing: border-box;
}

.dots {
  position: relative;
}

.dot {
  position: absolute;
  display: flex;
  justify-content: space-around;
  align-items: center;
  box-sizing: border-box;
  padding: 0 14rpx;
  bottom: 22rpx;
  right: 24rpx;
  width: 106rpx;
  height: 40rpx;
  border-radius: 20rpx;
  background: rgba(0, 0, 0, 0.3);
  color: #fff;
  font-size: 24rpx;
}

.dotImg {
  width: 20rpx;
  height: 16rpx;
}

.container {
  width: 92.5%;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  overflow-x: hidden;
}
.facility{
  width: 100%;
  height: auto;
  max-height: 160rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  margin-top: 60rpx;
}
.facilityTag{
  width:25%;
  overflow: hidden;
  white-space: nowrap;
  display: flex;
  align-items: center;
  height: 40rpx;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  justify-content:flex-start ;
}
.facilityText{
  max-width: 60%;
}
.orange{
  width: 22rpx;
  height: 16rpx;
  margin-right: 10rpx;
}
.lastHidden{
  width: 100%;
  height: 128rpx;
  background: transparent;
}
.title {
  font-size: 40rpx;
  margin: 40rpx 0 20rpx;
  display: -webkit-box;
  text-overflow: -o-epllipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.tags {
  width: 100%;
  overflow: hidden;
  height: 32rpx;
}

.tag {
  background: #fbf0e7;
  float: left;
  text-align: center;
  color: #F1612A;
  font-size: 20rpx;
  padding: 2rpx 14rpx;
  margin-right: 8rpx;
  border-radius: 22rpx;
}

.tag:last-child {
  margin-right: 0;
}

.surroundings {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  background:rgba(249,249,249,1);
  border-radius:4rpx;
  padding: 20rpx 0;
  margin-top: 32rpx;
}

.surrounding {
  width: 50%;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-left: 48rpx;
  font-weight: 400;
  color: rgba(71, 71, 71, 1);
  line-height: 50rpx;
  font-size: 28rpx;
  position: relative;
}
.surrounding::after{
  content: '';
  width: 10rpx;
  height: 10rpx;
  border-radius: 100%;
  position: absolute;
  left: 25rpx;
  top: 21rpx;
  background:rgba(216,216,216,1);
}
.heng{
  width:100%;
  height:12rpx;
  margin-top: 44rpx;
  background:rgba(249,249,249,1);
}
.landlordTitle {
  margin-top: 40rpx;
  font-size: 32rpx;
  font-weight: 500;
}

.landlordContainer {
  border-radius: 6rpx;
  border: 2rpx solid rgba(245, 245, 245, 1);
  width: 100%;
  height: 216rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.landlordTop {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.landlordAvator {
  width: 116rpx;
  height: 104rpx;
  margin-left: 38rpx;
  margin-top: 18rpx;
}

.landlordRight {
  width: 75.6%;
  height: 76rpx;
  display: flex;
  margin-top: 34rpx;
  flex-direction: column;
  flex: 1;
  box-sizing: border-box;
  padding-left: 40rpx;
  margin-left: 14rpx;
  border-left: 2rpx solid rgba(249, 249, 249, 1)
}

.landlordTags {
  width: 100%;
  overflow: hidden;
  margin-top: 4rpx;
  height: 28rpx;
}

.landlordTag {
  font-size: 20rpx;
  background: rgba(240, 240, 240, 1);
  border-radius: 7px;
  float: left;
  padding: 0rpx 14rpx;
  box-sizing: border-box;
  text-align: center;
  margin-right: 6rpx;
  display: block;
  font-weight: 400;
  color: rgba(185, 185, 185, 1);
}

.landlordTag:last-child {
  margin-right: 0;
}

.landlordBottom {
  display: flex;
  flex-direction: row;
  margin-top: 16rpx;
  box-sizing: border-box;
  padding-left: 44rpx;
  align-items: center;
}

.landlordTxt {
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  line-height: 50rpx;
  color: rgba(29, 29, 29, 1);
}

.landlordTxt:last-child {
  margin-left: 104rpx;
}

.lanlordValue {
  font-size: 36rpx;
  font-weight: 500;
}

.checkInNoticeTitle {
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(29, 29, 29, 1);
  line-height: 44rpx;
  margin: 40rpx 0 20rpx;
}

.checkInNoticeLi {
  font-size: 24rpx;
  line-height: 34rpx;
  font-weight: 400;
  display: flex;
  flex-direction: row;
}

.checkInNoticeLabel {
  color: rgba(185, 185, 185, 1);
}

.claim {
  color: rgba(241, 97, 42, 1);
}

.checkInNoticeTags {
  width: 100%;
  box-sizing: border-box;
  padding-left: 120rpx;
  font-size: 24rpx;
  font-weight: 400;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-bottom: 20rpx;
}

.checkInNoticeTag {
  display: flex;
  justify-content: flex-start;
  height: 34rpx;
  align-items: center;
  line-height: 34rpx;
  margin-right: 34rpx;
}

.checkInNoticeTag:last-child {
  margin-right: 0;
}

.symbol {
  width: 22rpx;
  height: 16rpx;
  margin-right: 6rpx;
}

.symbolFalse {
  width: 16rpx;
  height: 16rpx;
  padding: 0 4rpx;
  margin-right: 6rpx;
}

.symbolFalseText {
  color: rgba(190, 195, 205, 1);
}

.fix-footer {
  position: fixed;
  z-index: 1;
  bottom: 0;
  width: 100%;
  height: 128rpx;
  line-height: 128rpx;
  display: flex;
  justify-content: space-between;
  background: rgba(255, 255, 255, 1);
  border-radius: 1px;
  border: 1px solid rgba(244, 239, 239, 1);
  align-items: center;
  box-sizing: border-box;
  padding-left: 42rpx;
  padding-right: 28rpx;
}

.fix-text {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  font-size: 20rpx;
  align-items: flex-end;
  position: relative;
}

.money {
  font-size: 44rpx;
  font-weight: 500;
  color: rgba(241, 97, 42, 1);
}

.daycount {
  font-size: 28rpx;
  margin-left: 20rpx;
  color: rgba(29, 29, 29, 1);
  position: relative;
  bottom: -6rpx;
}

.daynight {
  position: relative;
  bottom: -6rpx;
}

.fix-btn {
  width: 240rpx;
  height: 92rpx;
  text-align: center;
  line-height: 92rpx;
  color: white;
  font-size: 28rpx;
  font-weight: 500;
  background: linear-gradient(180deg, rgba(231, 93, 40, 1) 0%, rgba(233, 68, 68, 1) 100%);
  box-shadow: 4rpx 4rpx 4rpx 0 rgba(198, 55, 35, 0.48);
  border-radius: 46rpx;
}
.fix-img{
  width: 76rpx;
  height: 76rpx;
}
.fix-house{
  font-size: 28rpx;
  color:rgba(185,185,185,1);
}
.fix-black{
  color: #000;
}
.houseDetail{
  width: 100%;
  height: auto;
  border-top: 2rpx solid rgba(249,249,249,1);
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin-top: 32rpx;
}
.houseDetailItem{
  display: flex;
  flex-direction: column;
  flex: 1;
  font-size: 24rpx;
  font-weight:400;
  color:rgba(71,71,71,1);
  line-height:34rpx;
  text-align: center;
  position: relative;
  margin-top: 24rpx;
}
.houseDetailItem::before{
  content: '';
  width: 4rpx;
  height: 56rpx;
  background:rgba(249,249,249,1);
  position: absolute;
  left: 0;
  top: 18rpx;
}
.houseDetailItem:first-child::before{
  display: none;
}
.houseNumber{
  font-size: 32rpx;
  font-weight:500;
  color:rgba(255,99,99,1);
  line-height:44rpx;
}
.detail-house{
  width: 100%;
  margin-top: 52rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding-left: 78rpx;
}
.detail-ol-mtp{
  margin-top: 14rpx;
}
.detail-ol{
  width: 50%;
  display: flex;
  line-height: 40rpx;
  justify-content: flex-start;
}
.grayText{
  color:rgba(185,185,185,1);
}
.navbar{
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin-top: 30rpx;
  font-size: 28rpx;
  color:rgba(189,189,189,1);
  position: relative;
}
.navbar .navbarLi{
  position: relative;
  height: 60rpx;
}
.navbar .navbarLi.active{
  color:#000;
}
.navbar .navbarLi.active::before{
  content: '';
  position: absolute;
  left: 18rpx;
  bottom: 0rpx;
  width: 72rpx;
  height: 10rpx;
  background:rgba(255,99,99,1);
  border-radius:6rpx;
}
.navbar-content{
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  background:rgba(255,255,255,1);
  border-radius:3px;
  border:1px solid rgba(245,245,245,1);
  box-sizing: border-box;
  padding: 40rpx 50rpx;
  font-size: 28rpx;
  line-height: 40rpx;
}
.plot{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  box-sizing: border-box;
  padding-left: 50rpx;
  margin-top: 26rpx;
  padding-bottom: 30rpx;
  border-bottom:2rpx solid rgba(249,249,249,1);
}
.plot-item{
  width: 33.33%;
  text-align: left;
}
.plot-title{
  font-size: 24rpx;
  color:rgba(185,185,185,1);
  line-height:34rpx;
  margin-bottom: 10rpx;
}
.plot-number{
  color:rgba(255,99,99,1);
  line-height:44rpx;
  font-size: 32rpx;
}
.plot-detail{
  width: 100%;
  display: flex;
  box-sizing: border-box;
  padding-left: 50rpx;
  justify-content: flex-start;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 8rpx;
}
.plot-detail-label{
  width: 140rpx;
  text-align: right;
  color:rgba(185,185,185,1);
}
.plot-detail-item{
  display: flex;
  justify-content: flex-start;
  width: 50%;
  margin-top: 20rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 24rpx;
}
.plot-detail-item:last-child{
  width: 100%;
}
.soldPlot{
  width: 100%;
  height: auto;
  margin-top: 30rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid rgba(249,249,249,1);
}
.soldPlot-title{
  font-size: 28rpx;
  line-height: 40rpx;
}
.soldPlot-text{
  display: flex;
  justify-content: space-between;
}
.soldPlot-price{
  font-size: 32rpx;
  color:rgba(255,99,99,1);
  line-height: 44rpx;
}
.soldPlot-average{
  margin-left: 34rpx;
  color:rgba(185,185,185,1);
  font-size: 24rpx;
}