@import "/app.wxss";
.mask{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  display: flex;
  justify-content: center;
  z-index: 201
}
.mask_content{
  margin-top: 300rpx;
  height: 386rpx;
}

.mask_content .house_image .image{
  width: 76rpx;
  height: 80rpx;
}
.mask_content .content{
  width: 624rpx;
  height: 256rpx;
  background: #FFFFFF;
  border-radius:  0 0 16rpx 16rpx;
}

.mask_content .content .text{
  text-align: center;
  font-size: 32rpx;
  height: 150rpx;
  color: #1D1D1D;
  line-height: 150rpx;
}
.content .btn{
  display: flex
}
.content .btn text{
  flex: 1;
  text-align: center;
  border-top: 1rpx solid #F2E9E6;
  height: 105rpx;
  line-height: 105rpx;
  color: #1D1D1D;
  font-weight: bold
}
.content .btn .right{
  color: #F1612A;
  border-left: 1rpx solid #F2E9E6;
}
