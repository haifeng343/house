.house_stat{
  margin: 0 28rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #E8E8E8;
  padding-bottom: 20rpx;
  color: #1D1D1D
}
.stat_card{
  margin: 24rpx 28rpx 40rpx;
}
.stat_card .stat_first{
  display: flex;
  color: #1D1D1D;
  flex-wrap: wrap;
  position: relative;
  justify-content:space-between;
}

.stat_first .stat_layout{
  width: 334rpx;
  height: 252rpx;
  margin-bottom: 26rpx;
  position: relative;
}
.stat_first .stat_layout image{
  width: 100%;
  height: 252rpx;
  border-radius: 8rpx
}
.stat_first .stat_layout .no_text{
  position: absolute;
  bottom: 46rpx;
  left: 50%;
  font-size:24rpx;
  font-weight:500;
  color:#FFFFFF;
  text-shadow:0px 1px 1px rgba(0,0,0,0.5);
  width: 96rpx;
  margin-left: -48rpx;
}
.stat_three .stat_layout .no_text{bottom: 20rpx;}
.stat_three .stat_normal{
  height: 382rpx;
}
.stat_three .stat_normal image{
  height: 382rpx;
}
.stat_three .stat_other{
  height: 180rpx;
}
.stat_three .stat_other image{
  height: 180rpx;
}
.stat_three .stat_ab{
  position: absolute;
  right: 0;
  bottom: 0;
}
.stat_two .stat_layout{
  height: 382rpx;
}
.stat_two .stat_layout image{
  height: 382rpx;
}
.stat_one .stat_layout{
  width: 100%;
  height: 382rpx;
}
.stat_one .stat_layout image{
  height: 382rpx;
}
.stat_layout .stat_tip{
  position: absolute;
  background: rgba(255, 255, 255, 0.8);
  width: 280rpx;
  height: 88rpx;
  bottom: 18rpx;
  left: 50%;
  margin-left: -140rpx;
  border-radius: 6rpx;
  font-size: 26rpx;
  color: #1D1D1D;
  text-align: center;
}
.stat_layout .tip{
  width: 112rpx;
  height: 44rpx;
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -56rpx;
  text-align: center;
  line-height: 42rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  background-repeat:no-repeat; 
  background-size:100% 100%;
  background-image:url('data:image/png;base64,');
}
.xz_layout .tip{
  background-image:url('data:image/png;base64,');
}
.mn_layout .tip{
  background-image:url('data:image/png;base64,');
}
.zg_layout .tip{
  background-image:url('data:image/png;base64,');
}
.stat_layout .stat_tip .price{
  margin-top: 8rpx;
}
.stat_layout .stat_tip .num{
  font-size: 22rpx;
  color: #9F9F9F
}

