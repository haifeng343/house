.mask{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.4);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 101
}
.top_image image{width: 580rpx;height: 248rpx;vertical-align:top}
.mask_out{
  width: 580rpx;
  height: 700rpx;
  background: #FFFFFF;
  border-radius: 0 0 16rpx 16rpx;
  position: relative;
}
.mask .mask_two,.mask .mask_one{height: 480rpx;}
.mask .mask_three{height: 548rpx}
.mask .mask_n_type{padding-bottom: 30rpx}
.mask_title{font-size:40rpx;font-weight:500;color: #4A4A4A;text-align: center}
.mask_content{font-size:28rpx;color: #9B9B9B;text-indent: 52rpx;margin: 4rpx 34rpx 0 52rpx}
.mask_mold{margin: 20rpx 28rpx 0;}
.mask_mold .mold_first{display: flex;flex-wrap: wrap;position: relative;justify-content:space-between;}
.mask_mold .mold_one{justify-content:center}
.mold{width:254rpx ;height: 188rpx;position: relative;}
.mold .mold_name{width:254rpx ;height: 188rpx;}
.mold image{width:254rpx;height: 188rpx;}
.mold .mold_name .num{font-size:36rpx;color: #FFFFFF;font-weight:500; position: absolute;  bottom: 40rpx;width:254rpx;text-align: center}
.mask_mold .mold_three .mold_normal{display: flex;height: 254rpx;justify-content: center;align-items: center}
.mask_mold .mold_three .mold_other{display: flex;height: 120rpx;justify-content: center;align-items: center}
.mask_mold .mold_three .mold_normal image{height: 254rpx;}
.mask_mold .mold_three .mold_other image{height: 120rpx;}
.mask_mold .mold_three .mold_other .num{font-size:32rpx;}
.mask_mold .mold_three .mold_ab{position: absolute;right: 0;bottom: 0;}

.mask_mold .mold_three .mold_normal .num{bottom: 56rpx;}
.mask_mold .mold_three .mold_normal .mold_name{height: 254rpx;}
.mask_mold .mold_three .mold_normal .line{width: 140rpx;height: 2rpx;position: absolute; background: #FFFFFF;left: 50%;top:50%;margin-left: -70rpx}
.mask_mold .mold_three .mold_normal .tj_logo{width: 126rpx;height: 32rpx;position: absolute;  top: 64rpx;left: 50%;margin-left: -63rpx}
.mask_mold .mold_three .mold_normal .xz_logo{width: 98rpx;height: 48rpx;position: absolute;  top: 64rpx;left: 50%;margin-left: -49rpx}
.mask_mold .mold_three .mold_normal .mn_logo{width: 48rpx;height: 32rpx;position: absolute;  top: 64rpx;left: 50%;margin-left: -24rpx}
.mask_mold .mold_three .mold_normal .zg_logo{width: 130rpx;height: 32rpx;position: absolute;  top: 64rpx;left: 50%;margin-left: -65rpx}

.mask_mold .mold_three .mold_other .num{bottom: 8rpx;}
.mask_mold .mold_three .mold_other .mold_name{height: 120rpx;}
.mask_mold .mold_three .mold_other .line{width: 100rpx;height: 2rpx;position: absolute; background: #FFFFFF;left: 50%;top:50%;margin-left: -50rpx}
.mask_mold .mold_three .mold_other .tj_logo{width: 126rpx;height: 32rpx;position: absolute;  top: 16rpx;left: 50%;margin-left: -63rpx}
.mask_mold .mold_three .mold_other .xz_logo{width: 98rpx;height: 48rpx;position: absolute;  top: 16rpx;left: 50%;margin-left: -49rpx}
.mask_mold .mold_three .mold_other .mn_logo{width: 48rpx;height: 32rpx;position: absolute;  top: 16rpx;left: 50%;margin-left: -24rpx}
.mask_mold .mold_three .mold_other .zg_logo{width: 130rpx;height: 32rpx;position: absolute;  top: 16rpx;left: 50%;margin-left: -65rpx}
.mask_mold .mold_three .mold_other .tj_n_logo{width: 174rpx;height: 52rpx;position: absolute;  top: 16rpx;margin-left: -63rpx}
.mask_mold .mold_three .mold_other .xz_n_logo{width: 130rpx;height: 66rpx;position: absolute;  top: 16rpx;margin-left: -63rpx}
.mask_mold .mold_three .mold_other .mn_n_logo{width: 84rpx;height: 60rpx;position: absolute;  top: 16rpx;margin-left: -63rpx}
.mask_mold .mold_three .mold_other .zg_n_logo{width: 190rpx;height: 56rpx;position: absolute;  top: 16rpx;margin-left: -63rpx}
.mask_out .mask_know{
  width: 482rpx;
  height: 92rpx;
  background:linear-gradient(181deg,rgba(233,68,68,1) 0%,rgba(231,93,40,1) 100%);
  color: #FFFFFF;
  margin: 18rpx auto 0;
  border-radius: 46rpx;
  text-align: center;
  line-height: 92rpx;
  box-shadow: 4rpx 4rpx 8rpx 0px rgba(198, 55, 35, 0.48);
  position: relative;
}
