  height: calc(100% - 128rpx);
  box-sizing: border-box;
  flex-wrap: wrap;
  border-right: 6rpx solid #f3f3f3;
  border-bottom: 2rpx solid #f4efef;
  background-color: #fff;
}

.position-content .position-middle .item.active {
  background-color: #fff;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAgAAAAICAYAAADED76LAAAAAXNSR0IArs4c6QAAAJNJREFUGBlNTTkOwjAQnHGBKPhBWroUfCESD6FASPkUVJQ8Il+gSEdLSYvAWFl2gi3swju7cxH5PftNk97pvAg4LI/jrdyDgMhPTIPBujhheO3bdRGwkDD7H8G7J3VKCoqtSTk9qYmGi5kxqJPuKJGaBB7evSNpjgF1qlvOmSS3q9N4zWKNLPJYOQv5Y6pfndU6wy8SFEQagjDcsgAAAABJRU5ErkJggg==);
