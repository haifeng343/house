@import '../../assets/styles/font.wxss';
@import '../../assets/styles/flex.wxss';

page {
  background-color: #fff;
}
.page{
  position: fixed;
  top: 102rpx;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 101;
}
.page .search-content {
  position: fixed;
  top: 112rpx;
  width: 100%;
  bottom: 142rpx;
  background-color: #fff;
  box-sizing: border-box;
  padding: 34rpx 0 0;
  z-index: 111;
}

.page .position-content {
  position: fixed;
  top: 192rpx;
  width: 100%;
  height: calc(100% - 192rpx);
  box-sizing: border-box;
  background-color: #f9f9f9;
  z-index: 111;
}

.search_item {
  width: 100%;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  border: none;
  height: auto;
  margin-bottom: 50rpx;
  padding: 0 44rpx;
  box-sizing: border-box;
  justify-content: flex-start;
}

.search_item--spread {
  color: #e75d28;
  text-align: center;
  margin: 24rpx 0;
}

.search_item--spread .icon-font {
  margin-top: 10rpx;
  font-size: 24rpx;
}

.search_item .position {
  flex: 1;
  font-size: 32rpx;
  color: #1d1d1d;
  line-height: 108rpx;
  font-weight: bold;
}

.search_item .positionByHand {
  flex: 0 0 auto;
  width: 140rpx;
  line-height: 108rpx;
  font-size: 24rpx;
  color: #1d1d1d;
  background: url(data:image/png;base64,) no-repeat center right;
  margin-right: 12rpx;
}

.search_item .inDate {
  flex: 0 0 auto;
  width: 186rpx;
}

.search_item .outDate {
  flex: 0 0 auto;
  width: 156rpx;
}

.search_item .total {
  flex: 1;
}

.search_item .number {
  flex: 0 0 auto;
  width: 154rpx;
  text-align: center;
}

.search_item .split {
  flex: 0 0 auto;
  width: 2rpx;
  height: 60rpx;
  align-items: center;
  background-color: #f4f4f4;
}

.search_item .inDate .text, .search_item .outDate .text,
.search_item .number .text {
  font-size: 20rpx;
  color: #bec3cd;
  line-height: 28rpx;
}

.search_item .inDate .date, .search_item .outDate .date,
.search_item .number .num {
  font-size: 32rpx;
  color: #1d1d1d;
  line-height: 44rpx;
  font-weight: bold;
}

.search_item .sort {
  position: absolute;
  top: 0;
  right: 66rpx;
  font-size: 24rpx;
  font-weight: bold;
}
.search-budget .search_item .sort {
  top: 34rpx;
}

.search_item .sort .uniE930 {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  color: #fff;
  line-height: 24rpx;
  font-size: 20rpx;
  text-align: center;
  background-color: #fff;
  border: 2rpx solid #e75d28;
  border-radius: 8rpx;
}

.search_item .sort .uniE930.active {
  color: #fff;
  background-color: #e75d28;
}

.search_item .scenic {
  flex: 1;
  font-size: 32rpx;
  color: #bec3cd;
  background: url(data:image/png;base64,) no-repeat center right;
  background-size: 14rpx 24rpx;
}

.search_item .scenic.active {
  color: #1d1d1d;
}

.search_item .title {
  width: 100%;
  font-size: 32rpx;
  padding-left: 28rpx;
  font-weight: bold;
  margin-bottom: 22rpx;
}

.search_item .title.icon_1 {
  background: url(data:image/png;base64,) no-repeat center left;
  background-size: 22rpx 28rpx;
  margin-bottom: 0;
}

.search_item .title.icon_2 {
  background: url(data:image/png;base64,) no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .houseType {
  flex: 0 0 auto;
  width: 136rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background: #f0f0f0;
  text-align: center;
  line-height: 40rpx;
  margin-right: 22rpx;
}

.search_item .houseType.active {
  background-color: #f1612a;
  color: #fff;
}

.search_item .title.icon_3 {
  background: url(data:image/png;base64,) no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .leaseType {
  flex: 0 0 auto;
  width: 156rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background: #f0f0f0;
  text-align: center;
  line-height: 40rpx;
  margin-right: 34rpx;
}

.search_item .leaseType.active {
  background-color: #f1612a;
  color: #fff;
}

.search_item .title.icon_4 {
  background: url(data:image/png;base64,) no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .title.icon_5 {
  background: url(data:image/jpeg;base64,) no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .equipments {
  font-size: 28rpx;
  color: #1d1d1d;
  width: 25%;
  text-align: center;
  margin-bottom: 18rpx;
}
.search_item .equipments .equipments-img {
  background: #FFF6F2;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin: 0 auto 6rpx;
  color: #1D1D1D;
  font-size: 46rpx;
  text-align: center;
  line-height: 96rpx;
}

.search_item .equipments.active .equipments-img {
  background: #F1612A;
  color: white;
}

.search-btn-back {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 142rpx;
  background: white;
  z-index: 112;
}
.search-btn {
  width: 480rpx;
  height: 92rpx;
  line-height: 92rpx;
  background: linear-gradient(180deg, rgba(231, 93, 40, 1) 0%, rgba(233, 68, 68, 1) 100%);
  box-shadow: 2px 2px 4px 0px rgba(198, 55, 35, 0.48);
  border-radius: 92px;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  margin: 30rpx auto 0;
}

.search-btn.position {
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  transform: translate(-50%);
  margin: 0;
}
.postion-city{
  position: fixed;
  top: 102rpx;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 104;
}
.postion-city .position-cityname{
  background: #ffffff;
  font-size: 32rpx;
  color: #1d1d1d;
  height: 90rpx;
  line-height: 90rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f4efef;
  border-top: 2rpx solid #f4efef;
  padding: 0 38rpx 0 30rpx;
  position: relative
}
.postion-city .city-change{
  width:154rpx;
  height:52rpx;
  background:#fff;
  border-radius:26rpx;
  border:1px solid #F1612A;
  font-size: 28rpx;
  color: #F1612A;
  text-align: center;
  position: absolute;
  right: 38rpx;
  top: 50%;
  transform: translate(0,-50%);
  line-height: 52rpx;
}
.postion-city .gray{
  color: #D6D6D6;
  border:1px solid #D6D6D6;
}
.position-content {
  display: flex;
}

.position-content .position-type {
  flex: 0 0 auto;
  width: 230rpx;
  height: calc(100% - 128rpx);
  box-sizing: border-box;
  background-color: #f9f9f9;
  flex-wrap: wrap;
  border-bottom: 2rpx solid #f4efef;
}

