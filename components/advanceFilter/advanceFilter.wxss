.position-content .position-middle .item.active.last {
  background-color: #fff5f1;
  background-image: url(data:image/png;base64,);
  background-size: 22rpx 16rpx;
  background-repeat: no-repeat;
  background-position: right center;
}

.position-content .position-last {
  flex: 3;
  width: 272rpx;
  height: calc(100% - 128rpx);
  box-sizing: border-box;
  flex-wrap: wrap;
  border-bottom: 2rpx solid #f4efef;
  background-color: #fff;
}

.position-content .position-last .item.active {
  background-color: #fff5f1;
  background-image: url(data:image/png;base64,);
  background-size: 22rpx 16rpx;
  background-repeat: no-repeat;
  background-position: 93% center;
}

.position-content .item {
  display: block;
  width: 100%;
  max-width: 514rpx;
  height: 92rpx;
  line-height: 92rpx;
  font-size: 28rpx;
  color: #1d1d1d;
  padding: 0 20rpx 0 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
}
