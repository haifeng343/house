@import "../../assets/styles/font.wxss";
@import "../../assets/styles/flex.wxss";

page {
  background-color: #fff;
}
.page {
  position: fixed;
  top: 102rpx;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 101;
}

.page .position-content {
  width: 100%;
  box-sizing: border-box;
  background-color: #f9f9f9;
  z-index: 111;
  height: 600rpx;
}

.price-content {
  height: 300rpx;
  padding-top: 40rpx;
  box-sizing: border-box;
}

.search-content {
  height: 600rpx;
  padding-top: 40rpx;
  box-sizing: border-box;
}

.search_item {
  width: 100%;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  border: none;
  height: auto;
  margin-bottom: 50rpx;
  padding: 0 44rpx;
  box-sizing: border-box;
  justify-content: flex-start;
}

.search_item--spread {
  color: #e75d28;
  text-align: center;
  margin: 24rpx 0;
}

.search_item--spread .icon-font {
  margin-top: 10rpx;
  font-size: 24rpx;
}

.search_item .position {
  flex: 1;
  font-size: 32rpx;
  color: #1d1d1d;
  line-height: 108rpx;
  font-weight: bold;
}

.search_item .positionByHand {
  flex: 0 0 auto;
  width: 140rpx;
  line-height: 108rpx;
  font-size: 24rpx;
  color: #1d1d1d;
  background: url(data:image/png;base64,)
    no-repeat center right;
  margin-right: 12rpx;
}

.search_item .inDate {
  flex: 0 0 auto;
  width: 186rpx;
}

.search_item .outDate {
  flex: 0 0 auto;
  width: 156rpx;
}

.search_item .total {
  flex: 1;
}

.search_item .number {
  flex: 0 0 auto;
  width: 154rpx;
  text-align: center;
}

.search_item .split {
  flex: 0 0 auto;
  width: 2rpx;
  height: 60rpx;
  align-items: center;
  background-color: #f4f4f4;
}

.search_item .inDate .text,
.search_item .outDate .text,
.search_item .number .text {
  font-size: 20rpx;
  color: #bec3cd;
  line-height: 28rpx;
}

.search_item .inDate .date,
.search_item .outDate .date,
.search_item .number .num {
  font-size: 32rpx;
  color: #1d1d1d;
  line-height: 44rpx;
  font-weight: bold;
}

.search_item .sort {
  position: absolute;
  top: 0;
  right: 66rpx;
  font-size: 24rpx;
  font-weight: bold;
}
.search-budget .search_item .sort {
  top: 34rpx;
}

.search_item .sort .uniE930 {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  color: #fff;
  line-height: 24rpx;
  font-size: 20rpx;
  text-align: center;
  background-color: #fff;
  border: 2rpx solid #e75d28;
  border-radius: 8rpx;
}

.search_item .sort .uniE930.active {
  color: #fff;
  background-color: #e75d28;
}

.search_item .scenic {
  flex: 1;
  font-size: 32rpx;
  color: #bec3cd;
  background: url(data:image/png;base64,)
    no-repeat center right;
  background-size: 14rpx 24rpx;
}

.search_item .scenic.active {
  color: #1d1d1d;
}

.search_item .title {
  width: 100%;
  font-size: 32rpx;
  padding-left: 28rpx;
  font-weight: bold;
  margin-bottom: 22rpx;
}

.search_item .title.icon_1 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
  margin-bottom: 0;
}

.search_item .title.icon_2 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .houseType {
  flex: 0 0 auto;
  width: 136rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background: #f0f0f0;
  text-align: center;
  line-height: 40rpx;
  margin-right: 22rpx;
}

.search_item .houseType.active {
  background-color: #f1612a;
  color: #fff;
}

.search_item .title.icon_3 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .leaseType {
  flex: 0 0 auto;
  width: 156rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background: #f0f0f0;
  text-align: center;
  line-height: 40rpx;
  margin-right: 34rpx;
}

.search_item .leaseType.active {
  background-color: #f1612a;
  color: #fff;
}

.search_item .title.icon_4 {
  background: url(data:image/png;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .title.icon_5 {
  background: url(data:image/jpeg;base64,)
    no-repeat center left;
  background-size: 22rpx 28rpx;
}

.search_item .equipments {
  font-size: 28rpx;
  color: #1d1d1d;
  width: 25%;
  text-align: center;
  margin-bottom: 18rpx;
}
.search_item .equipments .equipments-img {
  background: #fff6f2;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin: 0 auto 6rpx;
  color: #1d1d1d;
  font-size: 46rpx;
  text-align: center;
  line-height: 96rpx;
}

.search_item .equipments.active .equipments-img {
  background: #f1612a;
  color: white;
}

.search-btn {
  border-top: solid 2rpx #f4efef;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background-color: #fff;
  height: 112rpx;
  width: 100%;
  margin-top: auto;
  position: relative;
}
.scroll_mask {
  width: 100%;
  height: 40rpx;
  position: absolute;
  top:-40rpx;
  left: 0;
  background: linear-gradient(rgba(255,255,255,0) 0%,rgba(255,255,255,1) 70%,rgba(255,255,255,1) 100%);
}

.button {
  width: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  box-sizing: border-box;
  height: 72rpx;
  font-weight: 500;
  border-radius: 36rpx;
  color: #fff;
  background-image: linear-gradient(180deg, #e75d28 0%, #e94444 100%);
}

.postion-content .position-cityname {
  background: #ffffff;
  font-size: 32rpx;
  color: #1d1d1d;
  height: 90rpx;
  line-height: 90rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f4efef;
  border-top: 2rpx solid #f4efef;
  padding: 0 38rpx 0 30rpx;
  position: relative;
}
.postion-content .city-change {
  width: 154rpx;
  height: 52rpx;
  background: #fff;
  border-radius: 26rpx;
  border: 1px solid #f1612a;
  font-size: 28rpx;
  color: #f1612a;
  text-align: center;
  position: absolute;
  right: 38rpx;
  top: 50%;
  transform: translate(0, -50%);
  line-height: 52rpx;
}
.postion-content .gray {
  color: #d6d6d6;
  border: 1px solid #d6d6d6;
}
.position-content {
  display: flex;
}

.position-content .position-type {
  flex: 0 0 auto;
  width: 230rpx;
  height: 600rpx;
  box-sizing: border-box;
  background-color: #f9f9f9;
  flex-wrap: wrap;
}

.position-content .item {
  position: relative;
}

.position-content .position-type .item.active {
  background-color: #fff;
  color: #f1612a;
}

.position-content .position-type .item.active::before {
  content: "";
  width: 10rpx;
  height: 10rpx;
  background-color: #f1612a;
  position: absolute;
  top: 50%;
  transform: rotate(45deg) translateY(-50%);
  left: 16rpx;
}

.position-content .position-middle {
  flex: 2;
  height: 600rpx;
  box-sizing: border-box;
  flex-wrap: wrap;
  background-color: #fff;
}

.position-content .position-middle .item:not(.last).active {
  background-color: #fff;
  color: #f1612a;
}

.position-content .position-middle .item.last.active {
  background-color: #fff5f1;
}

.position-content .position-middle .item:not(.last).active::before {
  content: "";
  width: 10rpx;
  height: 10rpx;
  background-color: #f1612a;
  position: absolute;
  top: 50%;
  transform: rotate(45deg) translateY(-50%);
  left: 16rpx;
}

.position-content .position-middle .item.last.active::after {
  font-family: "iconfont" !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  content: "\e930";
  color: #f1612a;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 16rpx;
  font-size: 24rpx;
}

.position-content .position-last {
  flex: 3;
  width: 272rpx;
  height: 600rpx;
  box-sizing: border-box;
  flex-wrap: wrap;
  background-color: #fff;
  border-left: solid 2rpx #f3f3f3;
}

.position-content .position-last .item.active {
  background-color: #fff5f1;
}

.position-content .position-last .item.active::after {
  font-family: "iconfont" !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  content: "\e930";
  color: #f1612a;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 16rpx;
  font-size: 24rpx;
}

.position-content .item {
  display: block;
  width: 100%;
  max-width: 520rpx;
  height: 92rpx;
  line-height: 92rpx;
  font-size: 28rpx;
  color: #1d1d1d;
  padding: 0 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
}
.sort-panel {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  padding-bottom: 20rpx;
}

.sort-list {
  display: flex;
  flex-direction: column;
}

.sort-list .sort-item {
  height: 92rpx;
  width: 100%;
  display: flex;
  align-items: center;
  background-color: #fff;
  font-size: 28rpx;
  box-sizing: border-box;
  padding-left: 60rpx;
  position: relative;
}
.sort-list .sort-item.active {
  background-color: #f9f9f9;
  color: #f1612a;
}
.sort-list .sort-item .uniE930 {
  right: 40rpx;
  position: absolute;
  font-size: 24rpx;
}
